{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-connection.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/simple-test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/diagnose.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/mongo-test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug-mongo.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test-student-route.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/module-test.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/direct-mongo-students.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/direct-mongo-payments.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/direct-mongo-dashboard.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/direct-mongo-dashboard",
      "dest": "/api/direct-mongo-dashboard.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/direct-mongo-payments",
      "dest": "/api/direct-mongo-payments.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/direct-mongo-students",
      "dest": "/api/direct-mongo-students.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/module-test",
      "dest": "/api/module-test.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/test-student-route",
      "dest": "/api/test-student-route.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/debug-mongo",
      "dest": "/api/debug-mongo.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/mongo-test",
      "dest": "/api/mongo-test.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/diagnose",
      "dest": "/api/diagnose.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/simple-test",
      "dest": "/api/simple-test.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/test-connection",
      "dest": "/api/test-connection.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/test/(.*)",
      "dest": "/api/test.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "public": true,
  "github": {
    "silent": true
  }
}